{
    "presets": [
        "react",
        "es2015",
        "stage-2", [
            "env",
            {
                "targets": {
                    "browsers": "last 2 versions"
                },
                "modules": false
            }
        ]
    ],
    "plugins": [
        "transform-react-jsx",
        "react-hot-loader/babel",
        "babel-plugin-transform-class-properties",
        "transform-es2015-parameters",
        "transform-es2015-destructuring",
        "transform-object-rest-spread"
    ]
}